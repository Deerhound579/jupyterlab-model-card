{
  "name": "jupyterlab-model-card",
  "version": "1.0.0",
  "description": "Generate model cards for machine learning models",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/Deerhound579/jupyterlab-ext-tut",
  "bugs": {
    "url": "https://github.com/Deerhound579/jupyterlab-ext-tut/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Sixian",
  "files": [
    "dist/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "patches"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deerhound579/jupyterlab-ext-tut.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w",
    "postinstall": "jlpm run patch-package"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/launcher": "^2.2.5",
    "@jupyterlab/notebook": "^2.2.6",
    "antd": "^4.6.5",
    "immer": "^7.0.14",
    "modified-python-program-analysis": "^1.0.0",
    "react-markdown": "^5.0.3",
    "use-immer": "^0.4.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.164",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
